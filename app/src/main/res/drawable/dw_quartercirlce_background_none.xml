<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <!-- 正三角 -->
        <rotate
            android:fromDegrees="50"
            android:pivotX="0%"
            android:pivotY="100%"
            android:toDegrees="50">
            <shape android:shape="oval">
                <solid android:color="@color/colorMain" />
            </shape>
        </rotate>
    </item>
</layer-list>
